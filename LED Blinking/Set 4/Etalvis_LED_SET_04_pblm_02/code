volatile long i;
volatile char *dira,*dirb,*outa,*outb;
dira=0x21; dirb=0x24;
*dira=0xff; *dirb=0xff;
outa=0x22; outb=0x25;  
while(1)
{
    *outa=0xff;
    for(i=0;i<450000;i++);
    *outa=0x00;
    for(i=0;i<450000;i++);
    *outb=0xff; 
    for(i=0;i<450000;i++);
    *outb=0x00;
    for(i=0;i<450000;i++);
  }	

}

void loop() {}
