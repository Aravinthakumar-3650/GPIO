void setup() 
{
  volatile long i,j;
  volatile char *dira,*dirb,*outa,*outb;
  dira=0x21; dirb=0x24;
  *dira=0xff; *dirb=0xff;
  outa=0x22; outb=0x25;  
  while(1)
  {

    *outa=0x55;
    for(i=0;i<450000;i++);
    *outa=0xAA;
    for(i=0;i<450000;i++);
    *outa=0x00;
    
    *outb=0xAA;
    for(i=0;i<450000;i++);
    *outb=0x55;
    for(i=0;i<450000;i++);
    *outb=0x00;  
  }
}

void loop() {}
