void setup() 
{
  volatile char *dira,*outa;
  volatile long i;
  dira=0x21;
  *dira=0xff;
  outa=0x22;
  
  while(1)
  {
   *outa=0x88;
   for(i=0;i<450000;i++);
   *outa=0x44;
   for(i=0;i<450000;i++);
   *outa=0x22;
   for(i=0;i<450000;i++);
   *outa=0x11;
   for(i=0;i<450000;i++);
  }
}
void loop() {}
