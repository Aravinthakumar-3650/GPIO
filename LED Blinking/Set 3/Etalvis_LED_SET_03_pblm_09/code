void setup() 
{
  volatile char *dira,*outa,increment;
  volatile long i,j;
  dira=0x21;
  *dira=0xff;
  outa=0x22;
  
  while(1)
  {
    increment=0;
    for(i=0;i<8;i++)
    {
      increment=increment + (1<<i);
      *outa=increment;
      for(j=0;j<450000;j++);
      *outa=0x00;
      for(j=0;j<450000;j++);
    }
    
  }
}
void loop() {}
