void setup() 
{
  volatile char *dira,*outa;
  volatile long i;
  dira=0x21;
  *dira=0xff;
  outa=0x22;
  
  while(1)
  {
   *outa=0x30;
   for(i=0;i<450000;i++);
   *outa=0x0C;
   for(i=0;i<450000;i++);
   *outa=0xC0;
   for(i=0;i<450000;i++);
   *outa=0x03;
   for(i=0;i<450000;i++);
  }
}
void loop() {}

