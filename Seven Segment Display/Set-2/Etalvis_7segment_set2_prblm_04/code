void setup() 
{
  volatile char *dira,*outa;
  volatile long i,j;
  dira=0x21;
  *dira=0xff;
  outa=0x22;

  while(1)
  {
    volatile char order[10]={0xbf,0x86,0xdb,0xcf,0xe6,0xed,0xfd,0x87,0xff,0xef};
    for(i=0;i<10;i++)
    {
      *outa=~(order[i]);
      for(j=0;j<450000;j++);
      
    }
  }
}
void loop() {}
