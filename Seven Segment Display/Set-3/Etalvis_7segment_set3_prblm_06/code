void setup() 
{
  volatile char *dira,*dirb,*dirf,*dirk;
  volatile char *outa,*outb,*outf,*outk;
  volatile long i,j,k;

  dira=0x21;*dira=0xff;
  dirb=0x24;*dirb=0xff;
  dirf=0x30;*dirf=0xff;
  dirk=0x107;*dirk=0xff;

  outa=0x22;
  outb=0x25;
  outf=0x31;
  outk=0x108;

  while(1)
  {
    *outa=0x5b;
    *outb=0x3f;
    *outf=0x5b;
    *outk=0x6d;
  }
}

void loop() {}

