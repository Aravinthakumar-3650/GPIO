void setup() 
{
  volatile char *dira,*dirb,x;
  volatile char *outa,*inb;

  dira=0x21;
  *dira=0xff;
  dirb=0x24;
  *dirb=0x00;

  outa=0x22;
  inb=0x23;

  while(1)
  {
    x=*inb;
    if(((x & 0x02)==0x02))
    {
    while(1)
    { 
    x=*inb;
    if(((x & 0x80)==0x80))
    {
      *outa=0x20;
      delay_();
     *outa=0x00;
    }
    }
    }
  }
}
